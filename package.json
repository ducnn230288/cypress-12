{
  "name": "cypress-12",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.39.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cucumber": "^6.0.7",
    "cypress": "^12.11.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "cy:run": "npx cypress run --env allure=true",
    "open": "npx cypress open",
    "format": "prettier --write \"cypress/**/*.ts\"",
    "lint": "eslint \"cypress/**/*.ts\" --fix",
    "allure:report": "allure generate ./allure-results --clean -o ./allure-report",
    "pretest": "rimraf allure-results && rimraf allure-report && rimraf cypress/screenshots",
    "test": "npm run pretest && npm run cy:run",
    "post-test": "npm run allure:report && npm run open-report",
    "open-report": "allure open"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
